FORMAT(DocDueDate,&apos;yyyy-MM-ddTHH:mm:ssZ&apos;) As DueDate,&#xd;
CASE WHEN APFRDCTY IN (4, 5, 6) &#xd;
			 THEN PdTot * -1 &#xd;
			 ELSE PdTot &#xd;
		END As OriginalAmount,&#xd;
CASE WHEN APFRDCTY IN (4, 5, 6) &#xd;
			 THEN PdTot * -1 &#xd;
			 ELSE PdTot &#xd;
		END As GrossAmount,&#xd;
DiscTknTot As DiscountAmount,&#xd;
CASE WHEN APFRDCTY IN (4, 5, 6) &#xd;
			 THEN (PdTot * -1) - DiscTknTot  &#xd;
			 ELSE PdTot - DiscTknTot &#xd;
		END As NetAmount,&#xd;
&apos;&apos; As PayeeNumber,&#xd;
COALESCE(nullif(INV.TRXDSCRN,&apos;&apos;),nullif(INVOPEN.TRXDSCRN,&apos;&apos;),nullif(CRM.TRXDSCRN,&apos;&apos;),&apos;&apos;) As Description,&#xd;
B.PONUMBER As PONumber,&#xd;
&apos;&apos; As CF1,&#xd;
&apos;&apos; As CF2,&#xd;
&apos;&apos; As CF3,&#xd;
&apos;&apos; As CF4,&#xd;
&apos;&apos; As CF5&#xd;
&#xd;
From PM10201 B Left join  PM30200 Chk on Ltrim(Rtrim(CHK.VCHRNMBR)) =Ltrim(Rtrim(b.PMNTNMBR))&#xd;
LEFT JOIN PM30200 INV ON B.VCHRNMBR = INV.VCHRNMBR AND INV.DOCTYPE = 1  AND Chk.VENDORID = INV.VENDORID&#xd;
