  &quot;description&quot;: &quot;New query for DB1${__time()}&quot;,&#xd;
  &quot;queryString&quot;: &quot;Select B.DOCNUMBR As Number,FORMAT(B.DOCDATE,&apos;yyyy-MM-ddTHH:mm:ssZ&apos;) As Date,FORMAT(DocDueDate,&apos;yyyy-MM-ddTHH:mm:ssZ&apos;) As DueDate,CASE WHEN APFRDCTY IN (4, 5, 6) THEN PdTot * -1 		 ELSE PdTot END As OriginalAmount,CASE WHEN APFRDCTY IN (4, 5, 6) THEN PdTot * -1 ELSE PdTot END As GrossAmount,DiscTknTot As DiscountAmount,CASE WHEN APFRDCTY IN (4, 5, 6) THEN (PdTot * -1) - DiscTknTot  		 ELSE PdTot - DiscTknTot END As NetAmount,&apos;&apos; As PayeeNumber,CHK.VCHRNMBR as PaymentNumber,COALESCE(nullif(INV.TRXDSCRN,&apos;&apos;),nullif(INVOPEN.TRXDSCRN,&apos;&apos;),nullif(CRM.TRXDSCRN,&apos;&apos;),&apos;&apos;) As Description,B.PONUMBER As PONumber,&apos;&apos; As CF1,&apos;&apos; As CF2,&apos;&apos; As CF3,&apos;&apos; As CF4,&apos;&apos; As CF5 From PM10201 B Left join  PM30200 Chk on Ltrim(Rtrim(CHK.VCHRNMBR)) =Ltrim(Rtrim(b.PMNTNMBR))LEFT JOIN PM30200 INV ON B.VCHRNMBR = INV.VCHRNMBR AND INV.DOCTYPE = 1  AND Chk.VENDORID = INV.VENDORID  LEFT JOIN PM30200 CRM ON B.VCHRNMBR = CRM.VCHRNMBR AND CRM.DOCTYPE = 5  AND Chk.VENDORID = CRM.VENDORID  LEFT JOIN PM20000 INVOPEN ON  B.VCHRNMBR = INVOPEN.VCHRNMBR AND  INVOPEN.DOCTYPE = 1  AND Chk.VENDORID = INVOPEN.VENDORID where CHK.VCHRNMBR = @PaymentNumber&quot;,&#xd;
 &#xd;
  &quot;variables&quot;: [&#xd;
